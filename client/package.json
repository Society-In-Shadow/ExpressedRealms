{
  "name": "expressedrealms.client",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "npx vite",
    "build": "npx run-p type-check \"build-only {@}\" --",
    "preview": "npx vite preview",
    "build-only": "npx vite build",
    "type-check": "npx vue-tsc --noEmit -p tsconfig.app.json --composite false",
    "lint": "npx eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "component-tests": "sudo chown -hR $(whoami) node_modules/; npx cypress run --component --browser chromium",
    "open-cypress": "sudo chown -hR $(whoami) node_modules/; npx cypress open",
    "vitest": "sudo chown -hR $(whoami) node_modules/; npx vitest --run",
    "vitest-pipeline": "npx vitest --coverage"
  },
  "dependencies": {
    "@he-tree/vue": "^2.9.4",
    "@primevue/themes": "^4.3.7",
    "@tiptap/extension-table": "^3.0.9",
    "@tiptap/extension-underline": "^3.0.9",
    "@tiptap/pm": "^3.0.9",
    "@tiptap/starter-kit": "^3.0.9",
    "@tiptap/vue-3": "^3.0.9",
    "@vueuse/core": "^13.9.0",
    "axios": "^1.11.0",
    "bootstrap": "^5.3.7",
    "bootstrap-icons": "^1.13.1",
    "date-fns": "^4.1.0",
    "md5": "^2.3.0",
    "pinia": "^3.0.3",
    "pinia-plugin-persistedstate": "^4.4.1",
    "primeflex": "^4.0.0",
    "primeicons": "^7.0.0",
    "primevue": "^4.3.7",
    "quill": "^2.0.3",
    "vee-validate": "^4.15.1",
    "vue": "^3.5.18",
    "vue-easy-dnd": "^2.2.2",
    "vue-router": "^4.5.1",
    "yup": "^1.7.0"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.12.0",
    "@stylistic/eslint-plugin-js": "^4.4.1",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/vue": "^8.1.0",
    "@tsconfig/node18": "^18.2.4",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.2.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitest/coverage-v8": "^3.2.4",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "cross-fetch": "^4.1.0",
    "cypress": "^14.5.4",
    "cypress-multi-reporters": "^2.0.5",
    "depcheck": "^1.4.7",
    "eslint": "^9.32.0",
    "eslint-plugin-cypress": "^5.1.0",
    "eslint-plugin-sonarjs": "^3.0.4",
    "eslint-plugin-vue": "^10.4.0",
    "jest-matchmedia-mock": "^1.1.0",
    "jsdom": "^26.1.0",
    "mocha-junit-reporter": "^2.2.1",
    "npm-run-all2": "^8.0.4",
    "sass": "^1.90.0",
    "ts-standard": "^12.0.2",
    "typescript": "~5.9.2",
    "vite": "^6.0.0",
    "vitest": "^3.2.4",
    "vue-tsc": "^3.0.5"
  }
