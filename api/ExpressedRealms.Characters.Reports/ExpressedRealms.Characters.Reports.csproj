    </Content>
  </ItemGroup>
  <ItemGroup>
    <Content Include="fonts\**\*.ttf">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Content>
  </ItemGroup>
</Project>
